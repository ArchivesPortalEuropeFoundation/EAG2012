<grammar xmlns="http://relaxng.org/ns/structure/1.0">
   <start>
      <ref name="ead"/>
   </start>
   <define name="ead">
      <element name="ead" ns="https://archivists.org/ns/ead/v4">
         <group>
            <ref name="control"/>
            <optional>
               <ref name="fileDesc"/>
            </optional>
            <ref name="archDesc"/>
         </group>
      </element>
   </define>
   <define name="control">
      <element name="control" ns="https://archivists.org/ns/ead/v4">
         <group>
            <interleave>
               <optional>
                  <attribute name="audience" ns="">
                     <choice>
                        <value type="token" datatypeLibrary="" ns="">external</value>
                        <value type="token" datatypeLibrary="" ns="">internal</value>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="id" ns="">
                     <data type="ID" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="target" ns="">
                     <data type="IDREFS"
                           datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
                  </attribute>
               </optional>
               <zeroOrMore>
                  <attribute>
                     <anyName>
                        <except>
                           <choice>
                              <nsName ns="https://archivists.org/ns/ead/v4"/>
                              <nsName ns=""/>
                           </choice>
                        </except>
                     </anyName>
                     <text/>
                  </attribute>
               </zeroOrMore>
            </interleave>
            <optional>
               <attribute name="base" ns="">
                  <data type="anyURI"
                        datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
               </attribute>
            </optional>
            <interleave>
               <optional>
                  <attribute name="languageOfElement" ns="">
                     <data type="NMTOKEN"
                           datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="scriptOfElement" ns="">
                     <data type="NMTOKEN"
                           datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
                  </attribute>
               </optional>
            </interleave>
            <optional>
               <attribute name="languageEncoding" ns="">
                  <choice>
                     <value type="token" datatypeLibrary="" ns="">iso639-1</value>
                     <value type="token" datatypeLibrary="" ns="">iso639-2</value>
                     <value type="token" datatypeLibrary="" ns="">iso639-3</value>
                     <value type="token" datatypeLibrary="" ns="">ietf-bcp-47</value>
                     <value type="token" datatypeLibrary="" ns="">otherLanguageEncoding</value>
                  </choice>
               </attribute>
            </optional>
            <optional>
               <attribute name="scriptEncoding" ns="">
                  <choice>
                     <value type="token" datatypeLibrary="" ns="">iso15924</value>
                     <value type="token" datatypeLibrary="" ns="">otherScriptEncoding</value>
                  </choice>
               </attribute>
            </optional>
            <optional>
               <attribute name="dateEncoding" ns="">
                  <choice>
                     <value type="token" datatypeLibrary="" ns="">iso8601</value>
                     <value type="token" datatypeLibrary="" ns="">otherDateEncoding</value>
                  </choice>
               </attribute>
            </optional>
            <optional>
               <attribute name="countryEncoding" ns="">
                  <choice>
                     <value type="token" datatypeLibrary="" ns="">iso3166-1</value>
                     <value type="token" datatypeLibrary="" ns="">otherCountryEncoding</value>
                  </choice>
               </attribute>
            </optional>
            <optional>
               <attribute name="repositoryEncoding" ns="">
                  <choice>
                     <value type="token" datatypeLibrary="" ns="">iso15511</value>
                     <value type="token" datatypeLibrary="" ns="">otherRepositoryEncoding</value>
                  </choice>
               </attribute>
            </optional>
            <optional>
               <attribute name="detailLevel" ns="">
                  <choice>
                     <value type="token" datatypeLibrary="" ns="">minimal</value>
                     <value type="token" datatypeLibrary="" ns="">basic</value>
                     <value type="token" datatypeLibrary="" ns="">extended</value>
                  </choice>
               </attribute>
            </optional>
            <attribute name="maintenanceStatus" ns="">
               <data type="token"
                     datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
            </attribute>
            <optional>
               <attribute name="publicationStatus" ns="">
                  <data type="token"
                        datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
               </attribute>
            </optional>
            <ref name="recordId"/>
            <ref name="maintenanceAgency"/>
            <ref name="maintenanceHistory"/>
            <optional>
               <ref name="fileDesc"/>
            </optional>
            <optional>
               <ref name="sources"/>
            </optional>
            <zeroOrMore>
               <choice>
                  <ref name="conventionDeclaration"/>
                  <ref name="languageDeclaration"/>
                  <ref name="localControl"/>
                  <ref name="localTypeDeclaration"/>
                  <ref name="otherRecordId"/>
                  <ref name="representation"/>
                  <ref name="rightsDeclaration"/>
               </choice>
            </zeroOrMore>
         </group>
      </element>
   </define>
   <define name="fileDesc">
      <element name="fileDesc" ns="https://archivists.org/ns/ead/v4">
         <group>
            <ref name="titleStatement"/>
